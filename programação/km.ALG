Algoritmo "semnome"

Var
 n1,km,hm,dm,cm,mm:real;

Inicio
  escreval("escreva um numero")
  leia(n1)
  
  km:= n1/1000
  hm:= n1/100
  dm:= n1/10
  cm:= n1/0.1
  mm:= n1/0.001
  
  escreval("o valor de",km)
  escreval("o valor de",hm)
  escreval("o valor de",dm)
  escreval("o valor de",cm)
  escreval("o valor de",mm)

Fimalgoritmo