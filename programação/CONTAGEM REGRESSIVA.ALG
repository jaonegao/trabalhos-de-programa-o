Algoritmo "contagem regressiva"

Var
   numero: inteiro;

Inicio
   escreval("escreva um numero para a contagem regressiva:",numero)
   leia(numero)


   enquanto numero>=0 faca
      escreva(numero,"")
      numero <- numero -1
   fimenquanto

   escreva(":fim da contagem regressiva")

Fimalgoritmo