Algoritmo "semnome"

Var
 area,largura,altura,latas:real;

Inicio
    escreval("escreva a largura e a altura da parede")
    leia(altura,largura)
    
    
    area:= altura*largura
    latas:= area/2
    
    escreval("o valor da area é",area)
    escreval("a quantidade de latas usada é",latas)

Fimalgoritmo